{
  "name": "sonos-api",
  "version": "1.0.0",
  "description": "Sonos HTTP API proxy for node-sonos-http-api",
  "main": "src/server/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/index.js",
    "start:dev": "ts-node src/server/index.ts",
    "stop": "pkill -f sonos-api-server",
    "dev": "supervisor -p 200 -RV --debug -x npm run start:dev"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/request-promise-native": "^1.0.17",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "morgan": "^1.9.1",
    "request": "2.88.0",
    "request-promise-native": "^1.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "4.17.1",
    "@types/morgan": "^1.7.37",
    "@types/node": "^10.0.3",
    "@types/request": "2.48.3",
    "supervisor": "0.12.0"
  }
}
